print "I will now count my chickens:"

#30 divided by 6 plus 25
print "Hens", 25 + 30 / 6

#25 mulltiplied by 3, modulo 4; taken away from 100
print "Roosters", 100 -25 * 3 % 4

print "Now I will count the eggs:"

#4 modulo 2, minus 1 divided by 4; plus 3,2,1,6 and minus 5
print 3 + 2 + 1 - 5 + 4 % 2 - 1 / 4 + 6

print "Is it true that 3 + 2 < 5 - 7?"

#checking if 3 plus 2 is less than 5 minus 7
print 3 + 2 < 5 - 7

# adding 3 to 2
print "What is 3 + 2?", 3 + 2
#subtracting 7 from 5
print "What is 5 - 7?", 5 - 7

print "Oh, that's why it's False."

print "How about some more."

#checking if 5 is greater an -2
print "Is it greater?", 5 > -2
#checking if 5 is greater than or equal to -2
print "Is it greater or equal?", 5 >= -2
#checking if 5 is less than or equal -2
print "Is it less or equal?", 5 <= -2
